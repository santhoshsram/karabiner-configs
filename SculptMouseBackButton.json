{
  "title": "Microsoft Sculpt Mouse",
  "rules": [
    {
      "description": "Microsoft Sculpt Mouse Button4 to Browser Back",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button4"
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_command"
              ]
            }
          ]
        }
      ]
    },
    {
       "description": "Microsoft Sculpt Mouse Button3 to Browser Forward",
       "manipulators": [
          {
             "type": "basic",
             "from": {
                "pointing_button": "button3"
             },
             "to": [
                {
                   "key_code": "right_arrow",
                   "modifiers": [
                      "left_command"
                   ]
                }
             ]
          }
       ]
    }
  ]
}

